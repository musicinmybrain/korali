PREFIX="(PREFIX)"
CXX="(CXX)"
CXXFLAGS="(CXXFLAGS)"
LDFLAGS="(LDFLAGS)"

$CXX $@ $CXXFLAGS $LDFLAGS -I$PREFIX/include -L$PREFIX/lib -lkorali -Wl,-rpath=$PREFIX/lib
