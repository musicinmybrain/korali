#include "modules/conduits/simple/simple.hpp"
#include "modules/engine/engine.hpp"
#include "modules/problems/base.hpp"
#include "modules/solvers/base.hpp"

void Korali::Conduit::Simple::requestEvaluation(std::vector<double> sample, size_t sampleId)
{
 _k->_problem->runModel(sample, targetId);
 double fitness = _k->_problem->evaluateTarget();

 _k->_modelEvaluationCount++;
 _k->_solver->processEvaluation(targetId, fitness);
}

