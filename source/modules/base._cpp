#include "modules/base.hpp"

// Module Include List

Korali::Base* Korali::Base::getModule(nlohmann::json& js)
{
 std::string moduleType = "Undefined";

 if (! isDefined(js, "['Type']")) Korali::logError("No module type provided.\n");

 moduleType = js["Type"].get<std::string>();
 eraseValue(js, "['Type']");

 Korali::Base* module = nullptr;

 // Module Selection List

 if (module == nullptr) Korali::logError("Unrecognized module: %s.\n", moduleType.c_str());

 return module;
}
