#include "korali.hpp"

Korali::Module* Korali::Module::getModule(nlohmann::json& js)
{
 std::string moduleType = "Undefined";

 if (! isDefined(js, "['Type']")) Korali::logError("No module type provided.\n");

 moduleType = js["Type"].get<std::string>();
 eraseValue(js, "['Type']");

 Korali::Module* module = nullptr;

 // Module List

 if (module == nullptr) Korali::logError("Unrecognized problem type provided: %s.\n", moduleType.c_str());

 return module;
}
