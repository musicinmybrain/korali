#include "modules/problems/direct/base.hpp"
#include "modules/engine/engine.hpp"

bool Korali::Problem::Direct::Base::isTargetFeasible(const std::vector<double>& target)
{
  for (size_t i = 0; i < target.size(); i++)
  {
    if (std::isfinite(target[i]) == false) return false;
    if (target[i] < _variables[i]->_lowerBound) return false;
    if (target[i] > _variables[i]->_upperBound) return false;
  }
  return true;
}
