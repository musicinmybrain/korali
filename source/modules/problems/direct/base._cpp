#include "modules/problems/direct/base.hpp"

bool Korali::Problem::Direct::Base::isSampleFeasible(Korali::Sample* sample)
{
 printf("Got hereA\n");
  for (size_t i = 0; i < sample->getSample().size(); i++)
  {
    printf("Var Value: %f\n", sample->getSample()[i]);
    if (std::isfinite(sample->getSample()[i]) == false) return false;
    if (sample->getSample()[i] < _k->_variables[i]->_lowerBound) return false;
    if (sample->getSample()[i] > _k->_variables[i]->_upperBound) return false;
  }
  printf("Got hereB\n");
  return true;
}
