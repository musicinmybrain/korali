#include "korali.hpp"

Korali::Conduit::Base* Korali::Conduit::Base::getConduit(nlohmann::json& js)
{
 std::string moduleType = "Undefined";

 if (! isDefined(js, "['Type']")) Korali::logError("No conduit type provided.\n");

 moduleType = js["Type"].get<std::string>();
 eraseValue(js, "['Type']");

 Korali::Conduit::Base* module = nullptr;

 // Module List

 if (module == nullptr) Korali::logError("Unrecognized conduit type provided: = %s.\n", moduleType.c_str());

 return module;
}
