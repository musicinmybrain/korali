#include "korali.hpp"

Korali::Solver::Base* Korali::Solver::Base::getSolver(Korali::Solver::Base* solver, nlohmann::json& js)
{
 std::string solverType = "Undefined";

 if (! isDefined(js, "['Type']")) Korali::logError("No solver type provided.\n");

 solverType = js["Type"].get<std::string>();

 // Solver list

 if (solver == nullptr) Korali::logError("Unrecognized solver type provided: ['Type'] = %s.\n", solverType.c_str());

 solver->setConfiguration(js);
 solver->initialize(solver);
 return solver;
}
