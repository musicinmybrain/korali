#include "korali.hpp"

Korali::Solver::Base* Korali::Solver::Base::getSolver(nlohmann::json& js)
{
 std::string moduleType = "Undefined";

 if (! isDefined(js, "['Type']")) Korali::logError("No solver type provided.\n");

 moduleType = js["Type"].get<std::string>();
 eraseValue(js, "['Type']");

 Korali::Solver::Base* module = nullptr;

 // Module List

 if (module == nullptr) Korali::logError("Unrecognized solver type provided: = %s.\n", moduleType.c_str());

 return module;
}
