#include "models/base.hpp"
#include "math.h"

namespace Korali { namespace Model {

class Likelihood : public Base
{
  private:

  public:

  double _likelihood;

  void setLikelihood(double x)
  {
    // Check for result correctness
   ((Likelihood*)_self)->_likelihood = x;
  }

  void setLogLikelihood(double x)
  {
      // Check for result correctness
   ((Likelihood*)_self)->_likelihood = exp(x);
  }
};

} } // namespace Korali::Model

