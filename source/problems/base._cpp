#include "korali.hpp"

Korali::Problem::Base* Korali::Problem::Base::getProblem(nlohmann::json& js)
{
 std::string moduleType = "Undefined";

 if (! isDefined(js, "['Type']")) Korali::logError("No problem type provided.\n");

 moduleType = js["Type"].get<std::string>();
 eraseValue(js, "['Type']");

 Korali::Problem::Base* module = nullptr;

 // Module List

 if (module == nullptr) Korali::logError("Unrecognized problem type provided: %s.\n", moduleType.c_str());

 return module;
}
