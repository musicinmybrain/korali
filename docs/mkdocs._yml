site_name: Korali
#repo_name: 'cselab/korali'
#repo_url: 'https://github.com/cselab/korali'
edit_uri: ''
nav:
  - Home: index.md
  - Installation:
     - 'Installation Guide': install/install.md
     - 'System Requirements': install/requirements.md
     - 'Release Notes': https://github.com/cselab/korali/blob/master/docs/RELEASE-NOTES
     - 'Build Status': install/build.md
  - Tutorials: tutorials.md
  - User Manual:
      - Basics: usage/basics.md
      - Problems:
      - Solvers:
      - Conduits:
          - Linked: usage/conduits/linked.md
          - External: usage/conduits/external.md
      - Models:
          - Simple: usage/models/simple.md
          - Rerefence: usage/models/reference.md
          - Constraint: usage/models/constraint.md
      - Results:
          - Accessing Results: usage/results/#accessing-results
          - Plotting Results: usage/results/#plotting-results
          - Resume Execution: usage/results/#resume-execution
  - Publications: publications.md
  - About Us: aboutus.md
theme:
    name: material
    palette:
        primary: 'light blue'
        accent: 'blue'
    font:
        text: 'Ubuntu'
        code: 'Ubuntu Mono'
    logo: 'images/logo.png'
    favicon: 'images/favicon.ico'
# Got logo from https://icons8.com/icon/3036/coral
markdown_extensions:
  - pymdownx.arithmatex
  - pymdownx.details
  - pymdownx.superfences
  - attr_list
  - admonition
  - codehilite
extra_css:
    - style.css
extra_javascript:
  - 'https://cdnjs.cloudflare.com/ajax/libs/mathjax/2.7.0/MathJax.js?config=TeX-MML-AM_CHTML'
  
google_analytics: ['UA-43580832-2', 'https://www.cse-lab.ethz.ch/korali/']
